{
  "name": "fb-text2html",
  "version": "2.1.1",
  "description": "simple utility that transforms Facebook graph data: (text, tags) => html",
  "author": "Erik Hagreis",
  "keywords": [
    "facebook",
    "graph",
    "util",
    "text2html"
  ],
  "repository": "https://github.com/erikhagreis/fb-text2html",
  "license": "MIT",
  "main": "./lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "build": "webpack",
    "lib": "babel src --out-dir lib --ignore spec.js,umd.js",
    "prepublish": "npm run build && npm run lib",
    "test": "jest"
  },
  "dependencies": {
    "autolinker": "^1.8.3",
    "lodash": "^4.17.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "lint-staged": "^6.1.1",
    "prettier": "^1.18.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
